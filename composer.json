        "anomaly/slug-field_type": "^3.0",
        "anomaly/text-field_type": "^3.0",
        "anomaly/select-field_type": "^3.0",
        "anomaly/boolean-field_type": "^3.0",
        "anomaly/multiple-field_type": "^3.0",
