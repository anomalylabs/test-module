    "keywords": [
        "streams",
        "test",
        "module"
    ],
    "homepage": "http://pyrocms.com/addons/anomaly/test-module",
    "license": "MIT",
    "authors": [{
            "name": "PyroCMS, Inc.",
            "homepage": "http://pyrocms.com/",
            "role": "Owner"
        },
        {
            "name": "Ryan Thompson",
            "homepage": "http://ryanthepyro.com/",
            "role": "Developer"
        }
    ],
    "support": {
        "docs": "https://pyrocms.com/documentation/test-module",
        "forum": "https://pyrocms.com/forum/channels/test-module",
        "slack": "https://pyrocms.com/slack",
